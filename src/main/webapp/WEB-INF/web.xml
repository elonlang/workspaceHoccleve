<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
      <servlet-name>tei-html</servlet-name>
      <servlet-class>tk.hocl.controllers.TEIHtml</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>transform</servlet-name>
      <servlet-class>tk.hocl.controllers.XSLTTransformer</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>tei-numbers</servlet-name>
      <servlet-class>tk.hocl.controllers.TEINumbers</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>include-interp</servlet-name>
      <servlet-class>tk.hocl.controllers.IncludeInterp</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>server-info</servlet-name>
      <servlet-class>tk.hocl.controllers.ServerInfo</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>ctable</servlet-name>
      <servlet-class>tk.hocl.controllers.CTable</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>ctable-html</servlet-name>
      <servlet-class>tk.hocl.controllers.CTableHTML</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>transform</servlet-name>
      <url-pattern>/transform</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>tei-html</servlet-name>
      <url-pattern>/tei-html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>tei-numbers</servlet-name>
      <url-pattern>/tei-numbers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>include-interp</servlet-name>
      <url-pattern>/include-interp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>server-info</servlet-name>
      <url-pattern>/server-info</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>ctable</servlet-name>
      <url-pattern>/ctable</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>ctable-html</servlet-name>
      <url-pattern>/ctable-html</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
      <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
